CREATE TABLE Cliente (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, DNI VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID))
CREATE TABLE PRODUCTO (ID INTEGER AUTO_INCREMENT NOT NULL, DTYPE VARCHAR(31), ACTIVO TINYINT(1) default 0, NOMBRE VARCHAR(255), TIPOPRODUCTO VARCHAR(255), VERSION INTEGER, CLIENTE_ID INTEGER, PESO DOUBLE, PRIMARY KEY (ID))
ALTER TABLE PRODUCTO ADD CONSTRAINT FK_PRODUCTO_CLIENTE_ID FOREIGN KEY (CLIENTE_ID) REFERENCES Cliente (ID)
